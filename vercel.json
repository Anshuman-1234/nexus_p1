{
    "version": 2,
    "builds": [
        {
            "src": "Backend/index.js",
            "use": "@vercel/node"
        },
        {
            "src": "Frontend/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(api|login|signup|return|issue|create-order|verify-payment)(.*)",
            "dest": "Backend/index.js"
        },
        {
            "src": "/(.*)",
            "dest": "Frontend/$1"
        }
    ]
}